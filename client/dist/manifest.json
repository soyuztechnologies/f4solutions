{
	"_version": "1.6.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "oft.fiori",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"anubhavService": {
				"uri": "/odata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": ["{{keywords1}}"]
		},
		"ach": "SAP-HR"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_bluecrystal"],
		"icons": {
			"icon": "sap-icon://retail-store"
		}
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "1.44",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.44"
				},
				"sap.m": {
					"minVersion": "1.44"
				},
				"sap.ui.layout": {
					"minVersion": "1.44"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id": "mystyle"
			}]
		},
		"models": {
			"local": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "models/mockData/sampledata.json",
				"preload": true
			},
			"": {
				"dataSource": "anubhavService",
				"preload": true
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "oft.fiori.i18n.i18n"
				},

				"preload": true,
				"async": true
			}
		},
		"rootView": {
			"viewName": "oft.fiori.view.MainApp",
			"type": "XML",
			"id": "idMainApp"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "oft.fiori.view"

			},
			"routes": [
					  {
				"pattern": "split",
				"name": "App",
				"target": ["App"],
				"subroutes": [{
						"pattern": "master",
						"name": "master",
						"targetControl":"__xmlview0--idSplitApp",
						"target": ["donald"],
						"subroutes": [{
								"pattern": "newlead",
								"name": "newlead",
								"target": ["donald", "newlead"]
							},
							{
									"pattern": "GSTInvoices",
									"name": "GSTInvoices",
									"target": ["donald", "GSTInvoices"]
							},
							{
									"pattern": "InvoiceBuilder",
									"name": "InvoiceBuilder",
									"target": ["donald", "InvoiceBuilder"]
							},
							{
									"pattern": "InquiryLookup",
									"name": "InquiryLookup",
									"target": ["donald", "InquiryLookup"]
							},
							{
								"pattern": "newreg",
								"name": "newreg",

								"target": ["donald", "newreg"]
							},
							{
								"pattern": "server",
								"name": "server",

								"target": ["donald", "server"]
							},
							{
								"pattern": "serverDetails",
								"name": "serverDetails",

								"target": ["donald", "serverDet"]
							},

							{
								"pattern": "batch",
								"name": "batch",

								"target": ["donald", "batch"]
							},
							{
								"pattern": "adminPanel",
								"name": "adminPanel",

								"target": ["donald", "adminPanel"]
							},
							{
								"pattern": "downloadQueries",
								"name": "downloadQueries",
								"target": ["donald", "downloadQueries"]
							},
							{
								"pattern": "dataMaintenance",
								"name": "dataMaintenance",
								"target": ["donald", "dataMaintenance"]
							},
							{
								"pattern": "subsSearch",
								"name": "subsSearch",
								"target": ["donald", "subsSearch"]
							},
							{
								"pattern": "analytical",
								"name": "analytical",

								"target": ["analytical", "analytical"]
							},
							{
								"pattern": "newCustomer",
								"name": "newCustomer",
								"target": ["analytical", "newCustomer"]
							},
							{
								"pattern": "serverPayment",
								"name": "serverPayment",
								"target": ["analytical", "serverPayment"]
							},
							{
								"pattern": "ServerOverview",
								"name": "ServerOverview",
								"target": ["analytical", "ServerOverview"]
							},
							{
								"pattern": "BankAccount",
								"name": "BankAccount",
								"target": ["donald", "BankAccount"]
							},
							{
								"pattern": "mobileView",
								"name": "mobileView",
								"target": ["donald", "mobileView"]
							},
							{
								"pattern": "dumps",
								"name": "dumps",
								"target": ["donald", "dumps"]
							},
							{
								"pattern": "newTemplate",
								"name": "newTemplate",
								"target": ["donald", "newTemplate"]
							},
							{
								"pattern": "accessProcessor",
								"name": "accessProcessor",
								"target": ["donald", "accessProcessor"]
							},
							{
								"pattern": "blacklisted",
								"name": "blacklisted",
								"target": ["donald", "blacklisted"]
							}




						]
					}

				]
			}],
			"targets": {
				"App":{
					"viewName": "App",
					"controlId": "idMainApp",
					"controlAggregation": "pages",
					"viewLevel": 1
				},
				"donald": {
					"viewName": "View1",
					"controlId":"__xmlview0--idSplitApp",
					"controlAggregation": "masterPages",
					"viewId": "idView1",
					"viewLevel": 1
				},
				"newlead": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "newLead",
					"viewId": "idNewLead",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"GSTInvoices":{
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "GSTInvoices",
					"viewId": "idGSTInvoices",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"InvoiceBuilder":{
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "InvoiceBuilder",
					"viewId": "idInvoiceBuilder",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"InquiryLookup": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "InquiryLookup",
					"viewId": "idInquiryLookup",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"blacklisted":{
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "blacklisted",
					"viewId": "idBlack",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"newreg": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "newreg",
					"viewId": "idnewreg",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"server": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "server",
					"viewId": "idserver",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"serverDet": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "ServerDetails",
					"viewId": "idserverDet",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"batch": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "batch",
					"viewId": "idbatch",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"adminPanel": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "adminPanel",
					"viewId": "idAdminPanel",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"downloadQueries": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "downloadQueries",
					"viewId": "idDownloadQueries",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"dataMaintenance": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "dataMaintenance",
					"viewId": "idDataMaintenance",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"subsSearch":{
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "subsSearch",
					"viewId": "idSubsSearch",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"analytical": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "analytical",
					"viewId": "idanalytical",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"newCustomer": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "newCustomer",
					"viewId": "idNewCustomer",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"serverPayment": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "serverPayment",
					"viewId": "idServerPayment",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"ServerOverview": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "ServerOverview",
					"viewId": "idServerOverview",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"BankAccount":{
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "BankAccount",
					"viewId": "idBankAccount",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"mobileView":{
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "mobileView",
					"viewId": "idMobileView",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"dumps":{
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "dumps",
					"viewId": "idDumps",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"newTemplate":{
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "newTemplate",
					"viewId": "idNewTemplate",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"accessProcessor":{
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "accessProcessor",
					"viewId": "idaccessProcessor",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				}
			}
		}
	}
}
